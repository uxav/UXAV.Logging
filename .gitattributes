###############################################################################
# Setup Git LFS.
###############################################################################
*.vtz filter=lfs diff=lfs merge=lfs -text
*.vta filter=lfs diff=lfs merge=lfs -text
*.vtp filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.swf filter=lfs diff=lfs merge=lfs -text
*.lpz filter=lfs diff=lfs merge=lfs -text
*.puf filter=lfs diff=lfs merge=lfs -text
*.dll filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text